package controllers;

public interface IUsuarioController {

	public String login(String username, String contrasena);
	
	public String register(String username, String contrasena, String nombre, String apellidos, String email, double saldo, boolean premium);
	
	public String pedir(String username);
	
	public String restarDinero(double nuevoSaldo, String username);
	
	public String modificar(String username, String nuevaContrasena, String nuevoNombre, String nuevosApellidos, String nuevoEmail, double saldo, boolean nuevoPremium);
}
